                const defectDescription = item.type === 'defect' ?
                    item.originalItem.description?.toLowerCase().includes(keyword) ?? false : false;
                const matchesKeyword = item.title.toLowerCase().includes(keyword) || defectDescription;
                if (!matchesKeyword)
                    return false;
            }
            // 실행 상태 필터 (테스트케이스만)
