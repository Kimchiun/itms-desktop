"use strict";
Object.defineProperty(exports, "__esModule", { value: true });
exports.calcDefectDensity = calcDefectDensity;
exports.calcProgressRate = calcProgressRate;
exports.aggregateWorkload = aggregateWorkload;
// 결함 밀도 계산
function calcDefectDensity(defectCount, totalCases) {
    return totalCases > 0 ? defectCount / totalCases : 0;
}
// 진행률 계산
function calcProgressRate(passCount, totalCases) {
    return totalCases > 0 ? passCount / totalCases : 0;
}
// 작업량 집계
function aggregateWorkload(executions) {
    const workload = {};
    executions.forEach(r => {
        workload[r.executed_by] = (workload[r.executed_by] || 0) + 1;
    });
    return workload;
}
