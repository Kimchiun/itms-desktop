describe('dashboard 집계 로직', () => {
    it('defect density 계산', () => {
        expect((0, dashboardStats_1.calcDefectDensity)(5, 100)).toBe(0.05);
        expect((0, dashboardStats_1.calcDefectDensity)(0, 100)).toBe(0);
        expect((0, dashboardStats_1.calcDefectDensity)(10, 0)).toBe(0);
    });
